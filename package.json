{
    "name": "logic-specification",
    "displayName": "Logic Specification",
    "description": "Syntax highlighting for logic specification",
    "publisher": "Henry-YSLin",
    "version": "0.0.2",
    "bugs": {
        "url": "https://github.com/Henry-YSLin/logic-specification/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Henry-YSLin/logic-specification.git"
    },
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "logicspec",
            "aliases": ["Logic Specification", "logicspec"],
            "extensions": [".logic"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "logicspec",
            "scopeName": "source.logicspec",
            "path": "./syntaxes/logicspec.tmLanguage.json"
        }],
        "snippets": [{
            "language": "logicspec",
            "path": "./snippets.json"
        }]
    }
}